            <!--亮度-->
            <Param name="Brightness" value="150" default="150" />
            <!--对比度-->
            <Param name="Contrast" value="255" default="255" />
            <!--色调-->
            <Param name="Hue" value="0" default="0" />
            <!--饱和度-->
            <Param name="Saturation" value="64" default="64" />
