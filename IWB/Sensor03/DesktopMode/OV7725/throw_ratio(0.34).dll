    <!--电子白板手指触控模式下的正常使用参数-->
    <NormalUsage-FingerTouchWhiteBoard>
        <!--光斑触发门限-->
        <Param name="YThreshold" value="205" default="205" />
        <!--自动屏蔽门限占光斑触发门限的百分比(50~100)-->
        <Param name="DynmicMaskThresholdPercentage" value="80" default="80" />
